{
    "name": "n8n-nodes-obsidian-vault-rest-api",
    "version": "1.0.9",
    "description": "n8n community node for Obsidian Vault REST API",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "logo.svg",
        "openapi.yaml"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/j-shelfwood/prj-obsidian-local-rest-api.git"
    },
    "keywords": [
        "n8n",
        "n8n-nodes",
        "obsidian",
        "community-node",
        "openapi",
        "n8n-community-node-package"
    ],
    "author": "j-shelfwood",
    "license": "MIT",
    "scripts": {
        "build": "tsc && cp src/openapi.json dist && cp logo.svg dist",
        "dev": "tsc --watch"
    },
    "devDependencies": {
        "@types/node": "^22.15.2",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@devlikeapro/n8n-openapi-node": "^0.1.4",
        "n8n-workflow": "^1.82.0",
        "yaml": "^2.7.1"
    },
    "n8n": {
        "n8nNodesApiVersion": 1,
        "nodes": [
            "dist/index.js"
        ],
        "credentials": [
            "dist/credentials/BearerAuth.credentials.js"
        ]
    }
}
